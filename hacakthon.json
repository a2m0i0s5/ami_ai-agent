    {
      "parameters": {
        "jsCode": "// Parse, compute priority, and prepare top 5 summary\nlet feedbackList = items.map(item => {\n    const cleanOutput = item.json.output.replace(/```json|```/g, '').trim();\n    const feedback = JSON.parse(cleanOutput);\n    feedback.priority = feedback.urgency * feedback.impact;\n    return feedback;\n});\n\n// Sort by priority descending\nfeedbackList.sort((a, b) => b.priority - a.priority);\n\n// Take top 5\nfeedbackList = feedbackList.slice(0, 5);\n\n// Prepare a clean Slack/Notion message\nlet message = \"üìù *Top 5 Feedback Items This Week*\\n\";\n\nfeedbackList.forEach((fb, index) => {\n    const numEmoji = [\"1Ô∏è‚É£\", \"2Ô∏è‚É£\", \"3Ô∏è‚É£\", \"4Ô∏è‚É£\", \"5Ô∏è‚É£\"][index];\n    message += `\\n${numEmoji} *${fb.topic}*\\n`;\n    message += `   ‚û§ *Summary:* ${fb.summary}\\n`;\n    message += `   ‚û§ *Sentiment:* ${fb.sentiment}\\n`;\n    message += `   ‚û§ *Urgency:* ${fb.urgency} | *Impact:* ${fb.impact} | *Priority:* ${fb.priority}\\n`;\n});\n\n// Return the message in n8n item\nreturn [{ json: { message } }];\n"
